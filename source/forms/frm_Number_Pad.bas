Public Function ConvertToDecimalFeet()
Dim intValue As Integer
Dim dblDec As Double
Dim txtValue2 As Double
          
    intValue = Int(dbltxtValue)
    dblDec = dbltxtValue - intValue
            
    dblDec = dblDec * 0.833
            
    dbltxtValue = intValue + Round(dblDec, 2)
   
    
End Function

